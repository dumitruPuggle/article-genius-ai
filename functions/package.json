{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions,firestore,pubsub",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest",
    "stop": "lsof -i TCP:8080 | grep LISTEN | awk '{print $2}' | xargs kill -9 && lsof -i TCP:8085 | grep LISTEN | awk '{print $2}' | xargs kill -9 && lsof -i TCP:8085 | grep LISTEN | awk '{print $2}' | xargs kill -9 && lsof -i TCP:5001 | grep LISTEN | awk '{print $2}' | xargs kill -9"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@sparticuz/chromium": "^109.0.6",
    "chrome-aws-lambda": "^10.1.0",
    "express": "4.18.2",
    "firebase": "^9.18.0",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.0",
    "openai": "^3.2.1",
    "puppeteer": "^19.8.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/puppeteer": "^7.0.4",
    "firebase-functions-test": "^3.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "private": true
}
